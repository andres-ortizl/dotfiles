{
  "mcpServers": {
    "memos": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "mcp-server-memos"
      ],
      "env": {
        "MEMOS_URL": "${MEMOS_URL}",
        "MEMOS_TOKEN": "${MEMOS_ACCESS_TOKEN}"
      }
    },
    "context7": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp@latest"
      ],
      "env": {
        "CONTEXT7_API_KEY": "${CONTEXT7_API_KEY}"
      }
    }
  }
}